require "spec"
require "../src/*"

describe "<%-= to_captlized(@json["exercise"].to_s) %>" do
<%- @json["cases"].as_a.each do |cases| %>
    <%= status()%> "<%-= cases["description"] %>" do
    <%= to_captlized(@json["exercise"].to_s) %>.<%= to_snake(cases["property"].to_s)%>(<%= cases["input"]["x"].to_s.to_f %>, <%= cases["input"]["y"].to_s.to_f %>).should eq(<%= cases["expected"]%>)
    end
<% end %>
end